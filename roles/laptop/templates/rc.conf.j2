# {{ ansible_managed }} on {{ ansible_date_time.date }}
clear_tmp_enable="YES"
syslogd_flags="-ss"
sendmail_enable="NONE"
hostname="{{ hostname }}"
ifconfig_em0="DHCP"
ifconfig_em0_ipv6="inet6 accept_rtadv"
wlans_iwn0="wlan0"
ifconfig_wlan0="country IE regdomain none WPA DHCP"
local_unbound_enable="YES"
sshd_enable="YES"
moused_enable="YES"
ntpd_enable="YES"
powerd_enable="YES"
powerd_flags="-a min -b min -i 60 -M 1200"
dumpdev="AUTO"

webcamd_enable="YES"
